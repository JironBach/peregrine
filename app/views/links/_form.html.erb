<%= form_for(@link) do |f| %>
  <div class="field">
    <%= f.text_field :amzn_url, placeholder: "Amazon URL"%>
  </div>
  <div class="field">
    <%= f.text_field :aff_tag, placeholder: "Affiliate Tag" %>
  </div>
  <div class="actions">
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>
